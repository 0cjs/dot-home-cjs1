#!/bin/sh
#
# Decode URL-encoded text.
#

sed -e '
s/%20/ /g
s/%2D/-/g
s/%2E/./g
s/%30/0/g
s/%31/1/g
s/%32/2/g
s/%33/3/g
s/%34/4/g
s/%35/5/g
s/%36/6/g
s/%37/7/g
s/%38/8/g
s/%39/9/g
s/%3A/:/g
s/%3B/;/g
s/%3C/</g
s/%3D/=/g
s/%3E/>/g
s/%3F/?/g
s/%40/@/g
s/%41/A/g
s/%42/B/g
s/%43/C/g
s/%44/D/g
s/%45/E/g
s/%46/F/g
s/%47/G/g
s/%48/H/g
s/%49/I/g
s/%4A/J/g
s/%4B/K/g
s/%4C/L/g
s/%4D/M/g
s/%4E/N/g
s/%4F/O/g
s/%50/P/g
s/%51/Q/g
s/%52/R/g
s/%53/S/g
s/%54/T/g
s/%55/U/g
s/%56/V/g
s/%57/W/g
s/%58/X/g
s/%59/Y/g
s/%5A/Z/g
s/%5B/[/g
s/%5C/\\\\/g
s/%5D/]/g
s/%5E/^/g
s/%5F/_/g
s/%60/`/g
s/%61/a/g
s/%62/b/g
s/%63/c/g
s/%64/d/g
s/%65/e/g
s/%66/f/g
s/%67/g/g
s/%68/h/g
s/%69/i/g
s/%6A/j/g
s/%6B/k/g
s/%6C/l/g
s/%6D/m/g
s/%6E/n/g
s/%6F/o/g
s/%70/p/g
s/%71/q/g
s/%72/r/g
s/%73/s/g
s/%74/t/g
s/%75/u/g
s/%76/v/g
s/%77/w/g
s/%78/x/g
s/%79/y/g
s/%7A/z/g
s/%7B/{/g
s/%7C/|/g
s/%7D/}/g
s/%7E/~/g

s/%24/%/g' "$@"
