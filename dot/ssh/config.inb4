Host git.cynic.net.
    ForwardAgent no
    ForwardX11 no

Host *.cynic.net.
    ForwardAgent yes

Host pssh.github.com.
    #   Connect via proxy specified in `*_proxy` env vars.
    HostKeyAlias ssh.github.com.
    HostName ssh.github.com.
    Port 443
    User git
    ProxyCommand connect-proxy %h %p
